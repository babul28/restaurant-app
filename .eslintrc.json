{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "no-underscore-dangle": "off",
        "no-restricted-globals": "off",
        "no-undef": "off",
        "no-prototype-builtins": "off",
        "no-console": "off",
        "import/prefer-default-export": "off"
    }
}